{
  "name": "tumoreread",
  "version": "1.0.0",
  "description": "TU More Read is a POS (point-of-sale) system used within bookstores that is used to sell books, manage financial transactions, control and check the total number of books within the store. TU More Read is developed from the MERN Point of Sale, so TU More Read will use MongoDB, Express, React, and Node.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "server": "npm run server --prefix backend",
    "client": "npm start --prefix frontend",
    "dev": "concurrently 'npm run server' 'npm run client'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suteye/TuMoreRead.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/suteye/TuMoreRead/issues"
  },
  "homepage": "https://github.com/suteye/TuMoreRead#readme",
  "dependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.19.1",
    "@testing-library/user-event": "^14.4.3",
    "chai": "^4.3.7",
    "concurrently": "^7.4.0",
    "mocha": "^10.1.0",
    "protractor": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "save-dev": "^0.0.1-security"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.19.3",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/react": "^13.4.0",
    "babel-jest": "^29.1.2",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "jest-html-reporter": "^3.7.0",
    "react-test-renderer": "^18.2.0"
  },
  "jest": {
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporter",
        {
          "pageTitle": "Test Report"
        }
      ]
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "verbose": true,
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
